   "source": [
    "from datetime import datetime\n",
    "\n",
    "dob = input(\"Enter date of birth (DD-MM-YYYY): \")\n",
    "\n",
    "dob_val = datetime.strptime(dob, \"%d-%m-%Y\")\n",
    "now = datetime.now()\n",
    "age = now.year - dob_val.year\n",
    "print(\"ypur age is:\", age, \"years\")"
   ]
